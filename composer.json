{
    "name": "adriceci/audit-center",
    "description": "A comprehensive Laravel package for audit logging with Vue.js frontend components",
    "keywords": [
        "laravel",
        "audit",
        "logging",
        "vue",
        "laravel-package",
        "audit-log",
        "activity-log"
    ],
    "type": "library",
    "license": "MIT",
    "support": {
        "source": "https://github.com/adriceci/audit-center",
        "issues": "https://github.com/adriceci/audit-center/issues"
    },
    "authors": [
        {
            "name": "Adrian Cecilia",
            "email": "adrian.cecilia4@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "AdriCeci\\AuditCenter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdriCeci\\AuditCenter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AdriCeci\\AuditCenter\\Providers\\AuditCenterServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-autoload-dump": [
            "@php -r \"file_exists('.phpstorm.meta.php') && unlink('.phpstorm.meta.php');\""
        ]
    }
}

